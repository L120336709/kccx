<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sundata.edu.dao.SystemInitDao">

    <!-- 查询学科 -->
    <select id="querySubjects" resultType="java.util.Map">
        select id, name from  subject
    </select>

    <!-- 查询学年 -->
    <select id="querySchoolYear" resultType="java.util.Map">
        select id, name from  school_year
    </select>

    <!-- 查询考试 -->
    <select id="queryExams" resultType="java.util.Map">
        select id, name from  achievement_exam
    </select>


    <!-- 查询学生 -->
    <select id="queryUsers" resultType="java.util.Map">
        select user_id as id, real_name as name from  userinfo
    </select>

</mapper>