<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sundata.edu.dao.SysDictDataDao">
    
    <resultMap type="com.sundata.edu.domain.SysDictData" id="BaseResultMap">
        <result property="dataId" column="data_id" />
        <result property="dataSort" column="data_sort" />
        <result property="dataLabel" column="data_label" />
        <result property="dataValue" column="data_value" />
        <result property="typeCode" column="type_code" />
        <result property="cssClass" column="css_class" />
        <result property="listClass" column="list_class" />
        <result property="isDefault" column="is_default" />
        <result property="status" column="status" />
        <result property="remark" column="remark" />
        <result property="created" column="created" />
        <result property="updated" column="updated" />
    </resultMap>

    <select id="getDicDataKeyValue" resultType="com.sundata.edu.domain.SysDictData">
        SELECT * from sys_dict_data where type_code = #{typeCode}
    </select>

    <select id="getDataLabelKeyValue" resultType="com.sundata.edu.domain.SysDictData">
        select * from sys_dict_data where type_code = #{typeCode}
    </select>
    

</mapper>